<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="1.16.1" author="novikovdd" context="prod">
        <loadData tableName="qr_batches" file="db/changelog/data/qr_batches_28.10.2025.csv" schemaName="${schema}">
            <column header="template" name="template" type="STRING"/>
            <column header="distribution_method" name="distribution_method" type="STRING"/>
            <column header="description" name="description" type="STRING"/>
        </loadData>
